    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.bryanmtz.randomanime",
      "buildNumber": "1.0.4",
      "config": {
        "googleMobileAdsAppId": "IOS-ADDMOB-APP-ID"
      }
    },
    "android": {
      "package": "com.bryanmtz.randomanime",
      "versionCode": 5,
      "config": {
        "googleMobileAdsAppId": "ANDROID-ADDMOB-APP-ID"
      }
    }
  }
}

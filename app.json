{
  "expo": {
    "name": "Anime Random",
    "slug": "random-anime",
    "platforms": ["ios", "android"],
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/iconios.png",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#fafafa"
    },
    "primaryColor": "#fafafa",
    "updates": {
